// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}
model Users {
  id Int @id @default(autoincrement())
  name String
  image String
  email String
  hobbies Hobbies[]
}
model Hobbies{
  id Int @id @default(autoincrement())
  name String
  image String
  active Boolean
  user Users @relation(fields: [userId], references: [id])
  userId Int
}
